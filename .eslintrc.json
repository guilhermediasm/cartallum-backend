{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-plusplus": "warn",
      "consistent-return": "off",
      "prettier/prettier": "error",
      "no-underscore-dangle": "warn"
    }
}
